{
        "AuthMe": {
            "commands": [
                {
                    "name": "/register",
                    "syntax": "/register <password> <password>",
                    "example": "/register MySecurePass123 MySecurePass123",
                    "description": "Registers a new account on the server. You must type your password twice."
                },
                {
                    "name": "/login",
                    "syntax": "/login <password>",
                    "example": "/login MySecurePass123",
                    "description": "Logs you into your account using your previously registered password."
                }
            ]
        },
    "EssentialsX": {
        "commands": [
            {
                "name": "/sethome", "syntax": "/sethome <name>", "example": "/sethome home1 - Sets a home named 'home1'.", "description": "Allows you to set a personal home location."
            },
            {
                "name": "/home", "syntax": "/home <name>", "example": "/home home1 - Teleports you to 'home1'.", "description": "Teleports you to a previously set home."
            },
            { "name": "/tpa", "syntax": "/tpa {playername}", "example": "/tpa Steve", "description": "Request to teleport to a player." },
            { "name": "/tpaccept", "syntax": "/tpaccept", "example": "/tpaccept", "description": "Accept a teleport request." }
        ]
    },
    "EconomyShopGUI": {
        "commands": [
            {
                "name": "/shop", "syntax": "/shop", "example": "Use /shop to open the economy shop.", "description": "Opens the shop where you can buy and sell items."
            },
            {
                "name": "/sellhand", "syntax": "/sellhand", "example": "Sell the item in your hand using /sellhand.", "description": "Sells the item you are currently holding."
            },
            { "name": "/sellgui", "syntax": "/sellgui", "example": "/sellgui", "description": "Sell multiple items at once." }
        
        ]
    },
    "Gsit": {
        "commands": [
            { "name": "/sit", "syntax": "/sit", "example": "/sit", "description": "Sit anywhere in the world." },
            { "name": "/lay", "syntax": "/lay", "example": "/lay", "description": "Lay down on the ground." },
            { "name": "/spin", "syntax": "/spin", "example": "/spin", "description": "Spin in place." },
            { "name": "/crawl", "syntax": "/crawl", "example": "/crawl", "description": "Crawl on the ground." },
            { "name": "/bellyflop", "syntax": "/bellyflop", "example": "/bellyflop", "description": "Lay down on the ground on your stomach." }
        ]
    },
    "InvisibleItemFrames": {
        "commands": [
            { "name": "/ITF Toggle Visibility", "syntax": "/ITF Toggle Visibility", "example": "/ITF Toggle Visibility", "description": "Toggles item frame visibility." },
            { "name": "/ITF Toggle Glow", "syntax": "/ITF Toggle Glow", "example": "/ITF Toggle Glow", "description": "Toggles item frame Glow." }
        ]
    }, "TradeSystem": {
        "commands": [
            { "name": "/trade", "syntax": "/trade <player>", "example": "/trade Alex", "description": "Send a trade request." },
            { "name": "/trade accept", "syntax": "/trade accept", "example": "/trade accept", "description": "Accept a trade request." }
        ]
    },
    "mcMMO": {
        "commands": [
            { "name": "/mcstats", "syntax": "/mcstats", "example": "/mcstats", "description": "Shows your mcMMO stats." },
            { "name": "/mctop", "syntax": "/mctop", "example": "/mctop", "description": "Displays top mcMMO players." },
            { "name": "/party", "syntax": "/party <name>", "example": "/party create Warriors", "description": "Create or join an mcMMO party." },
            { "name": "/repair", "syntax": "/repair", "example": "/repair", "description": "Repairs items if you have enough levels." },
            { "name": "/inspect", "syntax": "/inspect <player>", "example": "/inspect Steve", "description": "Inspect another playerâ€™s mcMMO stats." }
        ]
    }
}
